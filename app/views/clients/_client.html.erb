<div id="<%= dom_id client %>">
  <p>
    <strong>Name:</strong>
    <%= client.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= client.email %>
  </p>

  <p>
    <strong>Mobile number:</strong>
    <%= client.mobile_number %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= client.address %>
  </p>

</div>
